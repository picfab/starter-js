{
    "env": {
        "browser": true,
        "es2021": true
    },
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },

    "extends": [
        "airbnb-base","prettier"
    ],
    "plugins":["prettier"],
    "rules": {
        "semi": [2, "never"],
        "no-console": ["error", { "allow": ["log","warn", "error"] }]

    }
}
